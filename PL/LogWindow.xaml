<Window x:Class="PL.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="LogWindow" Height="640" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Design/LogWindow/Bacground.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <TextBox x:Name="logInBox" HorizontalAlignment="Center" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="280" Height="33" Opacity="100" Margin="0,279,0,0" />
        <TextBlock x:Name="errorBox" HorizontalAlignment="Left" Margin="260,312,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="238" Height="15" FontSize="10" Foreground="Red" />
        <Image x:Name="pack___siteoforigin_____Admin_png" Source="/Design/LogWindow/Admin.png" MouseLeftButtonDown="loginAdmin" Margin="-99,179,-99,383"  />
        <Image x:Name="pack___siteoforigin_____Log_in_png" Margin="287,340,291,220"  Source="/Design/LogWindow/Log in.png" MouseLeftButtonDown="logIn"/>
        <Image x:Name="pack___siteoforigin_____Register_png"  Margin="288,507,302,57"    Source="/Design/LogWindow/Register.png" MouseLeftButtonDown="Register"/>
    </Grid>
</Window>
