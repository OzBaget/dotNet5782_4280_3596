<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="800">
    <Grid Margin="0,4,0,-4">
        <Grid.Background>
            <ImageBrush ImageSource="/Design/LogWindow/Bacground.png" Stretch="UniformToFill"/>
        </Grid.Background>

        <Image x:Name="CustomerList" Cursor="Hand" Source="/Design/MainWindow/Customer list.png" MouseLeftButtonDown="viewCustomerlList" Margin="-99,161,-99,401"  />
        <Image x:Name="DroneList" Cursor="Hand" Source="/Design/MainWindow/Drone list.png" MouseLeftButtonDown="ViewDroneList" Margin="-99,253,-99,309"  />
        <Image x:Name="ParcelList" Cursor="Hand" Source="/Design/MainWindow/Parcel list.png" MouseLeftButtonDown="viewParcelList" Margin="-99,347,-99,215"  />
        <Image x:Name="StationList" Cursor="Hand" Source="/Design/MainWindow/Station list.png" MouseLeftButtonDown="viewStationlList" Margin="-99,439,-99,123"  />


    </Grid>
</Window>
