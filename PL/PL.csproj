<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\AlfaSlabOne-Regular.ttf" />
    <None Remove="Fonts\Cotton Butter.ttf" />
    <None Remove="Fonts\Long_Shot.ttf" />
    <None Remove="Fonts\Rabble Rouser.ttf" />
    <None Remove="Fonts\Storytime.ttf" />
    <None Remove="Fonts\VintageKing.ttf" />
    <None Remove="Images\9752136.jpg" />
    <None Remove="Images\ClockSelector.png" />
    <None Remove="Images\downbackgroundDroneViewList.png" />
    <None Remove="Images\DroneView.png" />
    <None Remove="Images\upBackgroundViewDroneList.png" />
    <None Remove="Images\WeightKg.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Fonts\AlfaSlabOne-Regular.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Fonts\Cotton Butter.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Fonts\VintageKing.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BL\BL.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Fonts\Storytime.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\9752136.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\ClockSelector.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\downbackgroundDroneViewList.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\DroneView.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\upBackgroundViewDroneList.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\WeightKg.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
