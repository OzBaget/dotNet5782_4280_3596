<Window x:Class="PL.ViewDrone"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="ViewDrone" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="ID" HorizontalAlignment="Left" Margin="150,50,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="ID" HorizontalAlignment="Left" Margin="41,46,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Model" HorizontalAlignment="Left" Margin="41,72,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Model" HorizontalAlignment="Left" Margin="150,76,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label2" Content="Max Wight" HorizontalAlignment="Left" Margin="41,98,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="MaxWight" HorizontalAlignment="Left" Margin="150,100,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label3" Content="Battery" HorizontalAlignment="Left" Margin="41,126,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Battery" HorizontalAlignment="Left" Margin="150,130,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label4" Content="Status" HorizontalAlignment="Left" Margin="41,151,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="150,153,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label5" Content="Location" HorizontalAlignment="Left" Margin="41,191,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="150,186,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label6" Content="Lat" HorizontalAlignment="Left" Margin="101,182,0,0" VerticalAlignment="Top" Height="25"/>
        <Label x:Name="label7" Content="Lng" HorizontalAlignment="Left" Margin="101,0,0,0" VerticalAlignment="Center"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="150,212,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label8" Content="Parcel" HorizontalAlignment="Left" Margin="514,46,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
