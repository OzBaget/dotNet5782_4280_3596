        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="ViewParcelList" Height="640" Width="800">
    <Grid>
        <ListBox x:Name="ParcellistBox" Margin="0,80,0,0"/>
        <Button x:Name="reset" Content="reset" HorizontalAlignment="Left" Margin="687,18,0,0" VerticalAlignment="Top" Height="45" Width="77"/>
        <Button x:Name="add" Content="add" HorizontalAlignment="Left" Margin="537,18,0,0" VerticalAlignment="Top" Height="45" Width="77"/>
        <ComboBox x:Name="Status" HorizontalAlignment="Left" Margin="79,30,0,0" VerticalAlignment="Top" Width="120"/>
